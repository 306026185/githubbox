{
  "name": "githubbox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start": "concurrently \"yarn serve\" \"yarn build:watch\"",
    "serve": "serve build",
    "prebuild": "rm -rf build",
    "build": "yarn generate-md --layout github --input ./src --output ./build",
    "build:watch": "watch \"yarn build\" src",
    "pub": "wrangler publish",
    "pub:prod": "wrangler publish --env production"
  },
  "devDependencies": {
    "concurrently": "5.1.0",
    "markdown-styles": "3.1.10",
    "serve": "11.3.0",
    "watch": "1.0.2"
  }
}
